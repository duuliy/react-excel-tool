{
  "name": "react-excel-tool",
  "version": "1.0.0",
  "description": "An online Excel tool",
  "author": "duuliy (715181149@qq.com)",
  "license": "MIT",
  "keywords": [
    "react",
    "reactjs",
    "excel",
    "tool",
    "excel-tool",
    "excelTool",
    "component"
  ],
  "scripts": {
    "start": "cross-env NODE_ENV=dev webpack serve --progress --config webpack.config.js",
    "build": "cross-env CLIENT_ENV=production NODE_ENV=production webpack --config webpack.config.js",
    "test": "rimraf coverage && cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watchAll"
  },
  "jest": {
    "collectCoverage": false,
    "coverageThreshold": {
      "global": {
        "statements": 90,
        "branches": 90,
        "functions": 90,
        "lines": 90
      }
    },
    "collectCoverageFrom": [
      "__tests__/**/*.{js,jsx,ts,tsx}",
      "!__tests__/**/*.test.{js,jsx,ts,tsx}"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "transformIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "moduleDirectories": [
      "node_modules",
      "__tests__",
      "src"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/enzyme.config.js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)?$": "babel-jest",
      "^.+\\.css$": "<rootDir>/testBuild/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/testBuild/fileTransform.js"
    },
    "testPathIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/testBuild/fileMock.js",
      "\\.(css|less)$": "<rootDir>/testBuild/styleMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.12.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.4.1",
    "add": "^2.0.6",
    "autoprefixer": "^8.2.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "dompurify": "^2.2.6",
    "enzyme": "^3.11.0",
    "file-loader": "^6.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^4.5.1",
    "jest": "^26.6.3",
    "less": "^4.1.0",
    "less-loader": "^7.2.1",
    "mini-css-extract-plugin": "^1.3.4",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.1.0",
    "progress-bar-webpack-plugin": "^2.1.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "4.44.2",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  }
}
